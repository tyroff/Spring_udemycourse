create table person(
    id int PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
    name varchar(250),
    age int
)

insert into person(name, age) values ('Person_1', 39);

create table item(
    id int primary key generated by default as identity,
    person_id int REFERENCES person(id) on delete set null,
    item_name varchar(100) not null
);

insert into item(person_id, item_name) VALUES (1, 'Book');
insert into item(person_id, item_name) VALUES (2, 'AirPods');
insert into item(person_id, item_name) VALUES (3, 'Iphone');
insert into item(person_id, item_name) VALUES (4, 'Kindle');
insert into item(person_id, item_name) VALUES (5, 'TV');
insert into item(person_id, item_name) VALUES (6, 'PlayStation5');