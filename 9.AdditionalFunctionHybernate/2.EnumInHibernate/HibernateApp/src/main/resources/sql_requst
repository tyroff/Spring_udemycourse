create table item(
    id int primary key generated by default as identity,
    person_id int REFERENCES person(id) on delete set null,
    item_name varchar(100) not null
);

insert into item(person_id, item_name) VALUES (4, 'Book');
insert into item(person_id, item_name) VALUES (4, 'AirPods');
insert into item(person_id, item_name) VALUES (4, 'Iphone');

ALTER TABLE person ADD COLUMN date_of_birth DATE;
ALTER TABLE person ADD COLUMN created_at TIMESTAMP;